#include <iostream>
using namespace std;


int Findindex_element(int arr[] , int size , int target ){
            for(int i = 0 ; i < size ; i++){
                if(arr[i]== target ){
                    return i ;
                }
                
            }    
                return -1;
}






int main(){
    int arr[] = {2 , 3 , 4 , 6 , 7 , 10};
    int target = 5 ;
    int size = sizeof(arr) / sizeof(arr[0]);
   
 int result =   Findindex_element(arr , size , target);
 
 if(result != -1  ){
     cout << "Element found at index " << result ;
 } else 
    cout << "Element not found ";
 
    
    
}
